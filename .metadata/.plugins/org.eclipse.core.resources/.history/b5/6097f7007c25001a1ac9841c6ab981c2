	public static int [] solution(int [] A, int K) 
    {
        int N = A.length;
        if (N==0)
            return A;
        if (K>=N)
            K %= N;	
        if (K==0)
            return A;
        int [] rotA = new  int [N];
        for (int i=0; i<N; i++)
            rotA[i] = (i<K) ? A[N+i-K] : A[i-K];
        return rotA;
    }
	
	public static int [] rightRotat(int [] A, int K) 
	{
		int N = A.length;
		int gcd = Math.
		System.out.print(A);

		return A;
		
	}

}
